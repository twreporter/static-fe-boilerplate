{
  "name": "@twreporter/static-fe-hedwig",
  "version": "1.0.0",
  "description": "A boilerplate for building hedwig static files",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --kill-others \"npm run start-webpack-dev-server\" \"npm run start-dev-server\"",
    "start-webpack-dev-server": "node ./webpack-dev-server.js",
    "start-dev-server": "nodemon ./src/server --exec babel-node",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack -p --config webpack.config.js --env.NODE_ENV=production  --progress --colors",
    "build:server": "NODE_ENV=production babel-node src/server.js",
    "lint": "eslint --config .eslintrc",
    "lint:staged": "lint-staged",
    "watch": "webpack --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-assets-import-to-string": "^1.0.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "lodash.assign": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.map": "^4.6.0",
    "lodash.padstart": "^4.6.1",
    "lodash.partition": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-swipeable": "^4.2.0",
    "styled-components": "^2.2.1",
    "velocity-react": "^1.3.3"
  }
}
